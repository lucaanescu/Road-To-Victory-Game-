"Mind": 0.0,
"Money": 0.0,
"Spirit": 0.0
}
text/autocolor_names=false
history/simple_history_enabled=true
history/simple_history_save=true

[display]

window/stretch/mode="canvas_items"
window/stretch/scale=2.0
window/vsync/vsync_mode=0

